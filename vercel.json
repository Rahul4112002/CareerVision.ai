{
    "builds": [
      {
        "src": "jobroleprediction/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb",
          "installCommand": "pip install -r requirements-vercel.txt"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "jobroleprediction/wsgi.py"
      }
    ]
  }